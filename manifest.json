{
    "manifest_version": 2,
    "name": "Emotes For Messenger",
    "description": "Twitch emotes for messenger",
    "version": "0.1",
    "content_scripts": [
      {
        "matches": ["*://*.messenger.com/*"],
        "js": ["jquery-3.4.1.min.js", "main.js"],
        "run_at": "document_end"
      }
    ],
    "permissions": ["storage"]
  }